{
    "name": "webgl-practise",
    "version": "1.0.0",
    "description": "WebGL study and practise",
    "author": "chengeheng",
    "license": "ISC",
    "scripts": {
        "lint-staged": "lint-staged",
        "prepare": "husky install",
        "dev": "cross-env NODE_ENV=development webpack server --config ./config/webpack.dev.ts",
        "build": "cross-env NODE_ENV=production BABEL_ENV=production webpack  --config ./config/webpack.prod.js",
        "start": "npm run dev"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/chengeheng/WebGL-Notes.git"
    },
    "devDependencies": {
        "@babel/core": "^7.21.4",
        "@babel/preset-env": "^7.21.4",
        "@babel/preset-react": "^7.18.6",
        "@babel/preset-typescript": "^7.21.4",
        "@pmmmwh/react-refresh-webpack-plugin": "^0.5.8",
        "babel-loader": "^9.1.2",
        "babel-preset-react-app": "^10.0.1",
        "copy-webpack-plugin": "^11.0.0",
        "core-js": "^3.30.0",
        "cross-env": "^7.0.3",
        "css-loader": "^6.7.1",
        "css-minimizer-webpack-plugin": "^4.2.1",
        "eslint": "^8.25.0",
        "eslint-config-react-app": "^7.0.1",
        "eslint-webpack-plugin": "^3.2.0",
        "html-webpack-plugin": "^5.5.0",
        "less-loader": "^11.1.0",
        "mini-css-extract-plugin": "^2.6.1",
        "postcss-loader": "^7.0.1",
        "postcss-preset-env": "^7.8.2",
        "react-refresh": "^0.14.0",
        "sass": "^1.55.0",
        "sass-loader": "^13.1.0",
        "style-loader": "^3.3.1",
        "stylus-loader": "^7.1.0",
        "ts-node": "^10.9.1",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0",
        "webpack-dev-server": "^4.11.1"
    },
    "dependencies": {
        "@commitlint/cli": "^17.5.1",
        "@commitlint/config-conventional": "^17.4.4",
        "@types/react": "^18.0.34",
        "husky": "^8.0.3",
        "lint-staged": "^13.2.1",
        "prettier": "^2.8.7",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.4.2",
        "typescript": "^5.0.4"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "bugs": {
        "url": "https://github.com/chengeheng/WebGL-Notes/issues"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "peerDependencies": {
        "react": ">=17.0.0",
        "react-dom": ">=17.0.0"
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "lint-staged": {
        "./src/**/*.{js,jsx,ts,tsx}": [
            "eslint",
            "prettier --write"
        ]
    }
}
